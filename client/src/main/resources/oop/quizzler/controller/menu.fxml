<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.quizzler.controller.MenuController">
   <children>
      <Button fx:id="back" layoutX="80.0" layoutY="160.0" mnemonicParsing="false" onAction="#switchToWelcome" text="Back" />
      <Button fx:id="play" alignment="TOP_CENTER" layoutX="53.0" layoutY="74.0" onAction="#switchToSelectQuiz" prefHeight="25.0" prefWidth="93.0" text="Select A Quiz" textAlignment="CENTER" />
      <Button fx:id="create" layoutX="47.0" layoutY="112.0" mnemonicParsing="false" onAction="#switchToNamingQuiz" text="Create New Quiz" textAlignment="CENTER" />
      <Label alignment="TOP_CENTER" layoutX="43.0" layoutY="24.0" text="Welcome to Quizzler!" />
   </children>
</AnchorPane>
